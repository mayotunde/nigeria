<div id="map" style="width: 700px;height: 500px"></div>

<script>
var map = L.map('map').setView([4.250, 2.500], 7);
mapLink = 
'<a href="http://openstreetmap.org">openstreetmap</a>,'+
'<a href="http://mapsnigeriainitiative.wordpress.com">mapsnigeriainitiative</a>';
L.tileLayer(
'http://{s}.tile.openstreetmap.org/{z}/{x}/{y}.png', {
attribution: ' &copy; ' + mapLink,
maxZoom: 22,
}).addTo(map);
var pointsCluster = L.markerClusterGroup();
$.getJSON("https://cdn.rawgit.com/mayotunde/cluster/master/spots.geojson", function(data){
var points = L.geoJson(data, {
onEachFeature: function(feature, layer){
layer.bindPopup(feature.properties.COUNTRY)
}}).addTo(pointsCluster);
});
   
var points = L.markerClusterGroup();
$.getJSON("https://cdn.rawgit.com/mayotunde/cluster/master/spots.geojson", function(data){
var spots = L.geoJson(data, {
pointToLayer: function(feature, layer){
var marker = new L.marker(latlng);
marker.bindPopup(feature.properties.COUNTRY);
return marker;
}}).addTo(points);
});
   
map.addLayer(pointsCluster);
</script>
